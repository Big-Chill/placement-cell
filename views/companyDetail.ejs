<!DOCTYPE html>
<html>
    <head>
        <title>Company Details</title>
        <link rel="stylesheet" type="text/css" href="/stylesheets/student.css">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
    </head>
    <body>
        <h1>Students applied</h1>
        <table class="table-primary table table-bordered">
            <thead>
                <tr>
                    <th>Student Name</th>
                    <th>Student Status</th>
                </tr>
            </thead>

            <tbody>
                <% interviews.forEach(function(interview) { %>
                    <tr>
                        <td>
                            <a href="/students/getStudent/<%= interview.student._id %>">
                                <%= interview.student.name %>
                            </a>
                        </td>
                        <td><%= interview.student.status %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </body>
</html>